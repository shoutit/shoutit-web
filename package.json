{
  "name": "shoutit-web",
  "version": "0.0.1",
  "description": "Shoutit web app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/shoutitcom/shoutit_webapp.git"
  },
  "scripts": {
    "start": "node index.js",
    "build": "NODE_ENV=production webpack -p --bail",
    "lint": "eslint app test",
    "test": "npm run test:mocha && npm run test:karma",
    "test:karma": "karma start --reporters mocha",
    "test:karma:watch": "karma start --auto-watch --no-single-run",
    "test:karma:cover": "karma start --reporters mocha --reporters coverage",
    "test:mocha": "mocha --reporter mocha-circleci-reporter --require ./test/setup.js --compilers js:babel-register --recursive test app/**/*.test.js",
    "test:watch": "npm test -- --watch --watch-extensions jsx",
    "test:single": "mocha --reporter mocha-circleci-reporter --require ./test/setup.js --compilers js:babel-register --recursive --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli cover -- _mocha --reporter mocha-circleci-reporter --require ./test/setup.js --compilers js:babel-register --recursive test app/**/*.test.js",
    "dev": "redis-server & DEBUG=shoutit,shoutit:* NODE_ENV=development npm start",
    "dev:built": "redis-server & DEBUG=shoutit,shoutit:* NODE_ENV=production npm start",
    "sprite": "sprity create app/styles/ app/res/img/res1x/*.png -s sprites.scss --margin 0 -b && rm app/styles/sprite.png"
  },
  "author": "",
  "license": "ISC",
