{
  "name": "shoutit-web",
  "version": "0.0.1",
  "description": "Shoutit web app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/shoutitcom/shoutit_webapp.git"
  },
  "scripts": {
    "start": "node built/index.js",
    "prebuild": "rimraf built",
    "build": "npm run build:server && npm run build:client && cp -r assets built/assets",
    "build:server": "babel index.js app.js -d built && babel app -d built/app",
    "build:client": "NODE_ENV=production webpack -p --bail",
    "build:translations": "babel-node ./scripts/translations.js",
    "dev": "DEBUG=shoutit,shoutit:* NODE_ENV=development babel-watch -D -w app/server -w app/services -w app/server.js -w assets/intl/translations/*.json app.js",
    "dev:built": "redis-server & DEBUG=shoutit,shoutit:* NODE_ENV=production npm start",
    "lint": "eslint app",
    "test": "npm run lint && npm run test:mocha",
    "test:karma": "karma start --reporters mocha",
    "test:karma:watch": "karma start --auto-watch --no-single-run",
    "test:karma:cover": "karma start --reporters mocha --reporters coverage",
    "test:mocha": "mocha --reporter mocha-circleci-reporter --require ./test/setup.js --compilers js:babel-register --recursive app/**/*.test.js",
    "test:watch": "npm run test:mocha -- --watch",
    "test:single": "mocha --reporter mocha-circleci-reporter --require ./test/setup.js --compilers js:babel-register --recursive --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli cover -- _mocha --reporter mocha-circleci-reporter --require ./test/setup.js --compilers js:babel-register --recursive app/**/*.test.js"
  },
  "author": "",
  "license": "ISC",
