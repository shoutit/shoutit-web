    "body-parser": "1.14.2",
    "buffer": "3.6.0",
    "classnames": "2.2.3",
    "compression": "1.6.1",
    "connect-redis": "2.1.0",
    "consolidate": "0.13.1",
    "cors": "2.7.1",
    "create-hash": "1.1.2",
    "csurf": "1.8.3",
    "debug": "2.2.0",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "fluxxor": "1.7.3",
    "gm": "1.21.1",
    "history": "1.17.0",
    "imagemin": "4.0.0",
    "jade": "1.11.0",
    "lodash": "3.10.1",
    "material-ui": "0.14.0-rc1",
    "method-override": "2.3.5",
    "moment": "2.11.1",
    "morgan": "1.6.1",
    "multer": "1.1.0",
    "newrelic": "1.25.1",
