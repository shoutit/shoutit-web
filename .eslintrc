{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": "eslint:recommended",
  "rules": {
    "arrow-spacing": 2,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [2, 2],
    "jsx-quotes": [1, "prefer-double"],
    "no-extra-bind": 2,
    "no-extend-native": 2,
    "no-native-reassign": 2,
    "no-lonely-if": 2,
    "no-spaced-func": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "one-var": [2,  "never"],
    "prefer-const": 2,
    "quotes": [2, "double"],
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-after-keywords": 2,

    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-uses-react": 2,
    "react/no-danger": 2,
    "react/no-unknown-property": 2,
    "react/self-closing-comp": 2,
    "react/sort-comp": 2
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  }
}
